#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n, m;
        cin >> n >> m;

        string x, s;
        cin >> x >> s;

        if (x.find(s) != string::npos) {
            cout << 0 << "\n";
            continue;
        }

        string cur = x;
        int answer = -1;

       
        for (int k = 1; k <= 10; k++) {
            cur += cur;  
            if (cur.find(s) != string::npos) {
                answer = k;
                break;
            }
        }

        cout << answer << "\n";
    }

    return 0;
}

